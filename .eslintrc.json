{
  "root": true,
  "env": {
    "browser": true,
    "es2023": true
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript", "google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "require-jsdoc": "off"
  },
  "globals": {
    "$": true
  }
}
